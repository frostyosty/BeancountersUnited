{
  "version": 2,
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/dist/assets/$1"
    },
    {
      "src": "/(.*\\.(?:ico|png|svg|jpg|jpeg|webmanifest|js))",
      "headers": { "cache-control": "public, max-age=86400" },
      "dest": "/dist/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}